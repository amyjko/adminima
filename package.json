{
	"name": "adminima",
	"version": "0.6.11",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"csv": "node csv2org.js",
		"start": "cp .env.local .env; supabase start",
		"stop": "cp .env.prod .env; supabase stop",
		"reset": "supabase db reset; npm run types",
		"diff": "supabase db diff --local --use-migra -f rename_me",
		"types": "supabase gen types typescript --local > src/database/database.types.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.48",
		"@supabase/supabase-js": "^2.45",
		"supabase": "^1.183",
		"@sveltejs/adapter-auto": "^3.2",
		"@sveltejs/kit": "^2.7",
		"@sveltejs/vite-plugin-svelte": "^4.0",
		"@types/uuid": "^9.0",
		"@typescript-eslint/eslint-plugin": "^8.10",
		"@typescript-eslint/parser": "^8.10",
		"eslint": "^9.13",
		"eslint-config-prettier": "^9.1",
		"eslint-plugin-svelte": "^2.45.1",
		"prettier": "^3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0",
		"svelte-check": "^4.0",
		"tslib": "^2.8",
		"typescript": "^5.6",
		"vite": "^5.4.4",
		"vitest": "^2.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0.5",
		"@sveltejs/adapter-vercel": "^5.4",
		"date-fns": "^4.1",
		"fast-csv": "^5.0.1",
		"uuid": "^10.0",
		"write-excel-file": "^2.0"
	}
}
