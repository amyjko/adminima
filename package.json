{
	"name": "adminima",
	"version": "0.6.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"csv": "node csv2org.js",
		"start": "cp .env.local .env; supabase start",
		"stop": "cp .env.prod .env; supabase stop",
		"reset": "supabase db reset; npm run types",
		"diff": "supabase db diff --local --use-migra -f rename_me",
		"types": "supabase gen types typescript --local > src/database/database.types.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28",
		"@supabase/supabase-js": "^2",
		"@sveltejs/adapter-auto": "^3.2",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1",
		"@types/uuid": "^9.0",
		"@typescript-eslint/eslint-plugin": "^6.0",
		"@typescript-eslint/parser": "^6.0",
		"eslint": "^8.28",
		"eslint-config-prettier": "^8.5",
		"eslint-plugin-svelte": "^2.30",
		"prettier": "^2.8",
		"prettier-plugin-svelte": "^2.10",
		"supabase": "^1.183.5",
		"svelte": "^4.2",
		"svelte-check": "^3.4",
		"tslib": "^2.6",
		"typescript": "^5.5.3",
		"vite": "^5.2",
		"vitest": "^1.5"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"date-fns": "^2.30.0",
		"fast-csv": "^5.0.1",
		"uuid": "^9.0.1"
	}
}
