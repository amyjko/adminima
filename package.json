{
	"name": "adminima",
	"version": "0.8.6",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"csv": "node csv2org.js",
		"start": "cp .env.local .env; supabase start",
		"stop": "cp .env.prod .env; supabase stop",
		"reset": "supabase db reset; npm run types",
		"diff": "supabase db diff --local --use-migra -f rename_me",
		"types": "supabase gen types typescript --local > src/database/database.types.ts",
		"outdated": "npx check-outdated --ignore-pre-releases --columns name,type,current,latest,changes"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1",
		"@supabase/supabase-js": "^2",
		"@sveltejs/kit": "^2",
		"@sveltejs/vite-plugin-svelte": "^6",
		"@types/uuid": "^10.0",
		"prettier": "^3",
		"prettier-plugin-svelte": "^3",
		"supabase": "^2",
		"svelte": "^5",
		"svelte-check": "^4",
		"tslib": "^2",
		"type-fest": "^5.0.0",
		"typescript": "^5",
		"vite": "^7",
		"vitest": "^4"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0",
		"@sveltejs/adapter-vercel": "^6",
		"@vercel/speed-insights": "^1.2.0",
		"date-fns": "^4",
		"fast-csv": "^5",
		"uuid": "^13",
		"write-excel-file": "^2"
	}
}
