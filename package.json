{
	"name": "adminima",
	"version": "0.7.16",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"csv": "node csv2org.js",
		"start": "cp .env.local .env; supabase start",
		"stop": "cp .env.prod .env; supabase stop",
		"reset": "supabase db reset; npm run types",
		"diff": "supabase db diff --local --use-migra -f rename_me",
		"types": "supabase gen types typescript --local > src/database/database.types.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1",
		"@supabase/supabase-js": "^2",
		"@sveltejs/adapter-auto": "^6",
		"@sveltejs/kit": "^2",
		"@sveltejs/vite-plugin-svelte": "^5",
		"@types/uuid": "^10.0",
		"@typescript-eslint/eslint-plugin": "^8",
		"@typescript-eslint/parser": "^8",
		"eslint": "^9",
		"eslint-config-prettier": "^10",
		"eslint-plugin-svelte": "^3",
		"prettier": "^3",
		"prettier-plugin-svelte": "^3",
		"supabase": "^2",
		"svelte": "^5",
		"svelte-check": "^4",
		"tslib": "^2",
		"typescript": "^5",
		"vite": "^6",
		"vitest": "^3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0",
		"@sveltejs/adapter-vercel": "^5",
		"@vercel/speed-insights": "^1.2.0",
		"date-fns": "^4",
		"fast-csv": "^5",
		"uuid": "^11",
		"write-excel-file": "^2"
	}
}
